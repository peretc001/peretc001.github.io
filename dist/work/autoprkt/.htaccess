RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f


RewriteCond %{REQUEST_FILENAME} !-d
 # если запрашиваемый объект с дописанным расширением php - файл
RewriteCond %{REQUEST_FILENAME}\.php -f
 # делаем замену с дописыванием .php
RewriteRule ^(.*)/$ $1.php

# car
RewriteRule ^car/Infiniti/prokat_Infiniti_EX35/$ 					/car/page.php?id=1 [NC,L]
RewriteRule ^car/Infiniti/prokat_Infiniti_FX35/$ 					/car/page.php?id=2 [NC,L]
RewriteRule ^car/Audi/prokat_Audi_A6/$ 								/car/page.php?id=3 [NC,L]
RewriteRule ^car/BMW/prokat_BMW_5/$ 								/car/page.php?id=4 [NC,L]
RewriteRule ^car/Lexus/prokat_Lexus_IS250/$ 						/car/page.php?id=5 [NC,L]
RewriteRule ^car/Lexus/prokat_Lexus_GS250/$ 						/car/page.php?id=6 [NC,L]





AddDefaultCharset UTF-8
AddType application/x-httpd-php .php .htm .html .phtml
ErrorDocument 404 /404.php
ErrorDocument 403 /404.php

RewriteCond %{HTTP_HOST} ^autoprkt.ru
RewriteRule (.*) http://www.autoprkt.ru/$1 [R=301,L]


